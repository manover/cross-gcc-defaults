#!/usr/bin/make -f
# -*- makefile -*-

#export DH_VERBOSE=1
%:
	dh $@

override_dh_auto_build:
override_dh_auto_configure:
override_dh_auto_test:

override_dh_auto_install:
	DEB_TARGET_ARCH="${DEB_TARGET_ARCH}" \
	  debian/install-links.sh

override_dh_clean:
	dh_clean
	rm -rf debian/tmp

control:
	DEB_TARGET_ARCH="${DEB_TARGET_ARCH}" HOST_LIST="${HOST_LIST}" \
	  debian/generate-control.sh

update-defaults:
	debian/update-defaults.sh
